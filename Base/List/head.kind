use Base/List/ as L/
use Base/Maybe/ as M/

// Safely retrieves the first element of a list.
// - xs: The input list.
// = Some x if the list is non-empty (where x is the first element),
//   None if the list is empty.
Base/List/head
: ∀(A: *)
  ∀(xs: (L/List A))
  (M/Maybe A)
= λA λ{
  #nil: #none{}
  #cons: λhead λtail #some{head}
}
